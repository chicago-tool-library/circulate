<% content_for :header do %>
  <%= breadcrumbs "Pickups", admin_pickups_path %>
  <%= index_header "Pickup ##{@pickup.id}" %>
<% end %>

<%= render @pickup %>

<div>
  <%= link_to "Edit this pickup", edit_admin_pickup_path(@pickup) %>
  <%= button_to "Destroy this pickup", [:admin, @pickup], method: :delete, class: "btn" %>
</div>
